{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/node_modules": true,
        "**/README.md": true,
        "**/test.js": true
    },
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit"
    },
    "editor.formatOnSave": true,
    "eslint.alwaysShowStatus": true,
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "jsx"
    ],
    "prettier.disableLanguages": [
        "js",
        "jsx"
    ],
    "cSpell.words": [
        "eller",
        "enhet",
        "enheter",
        "flaske",
        "flere",
        "fordrukne",
        "gsap",
        "hver",
        "Innstillinger",
        "Logg",
        "lokalt",
        "pirat",
        "Sjømann",
        "slukke",
        "spille",
        "spillmodus",
        "tilbake",
        "Tweens",
        "tørst",
        "Velg",
        "Ønsker"
    ]
}